{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "_/*": [
        "src/*"
      ],
      "_public/*": [
        "public/*"
      ],
      "_main/*": [
        "src/main/*"
      ],
      "_models/*": [
        "src/models/*"
      ],
      "_renderer/*": [
        "src/renderer/*"
      ],
      "_utils/*": [
        "src/utils/*"
      ]
    },
    "allowJs": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "esnext",
    "typeRoots": [
      "./node_modules/@types",
      "./src/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}